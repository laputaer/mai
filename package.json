{
  "name": "mai",
  "version": "1.4.1",
  "description": "mai codebase",
  "main": "index.js",
  "scripts": {
    "test": "mocha components/tests/",
    "report": "babel-istanbul cover _mocha -- -R spec components/tests/",
    "start": "NODE_ENV=production node index.js",
    "setup": "npm install && npm run build:css && npm run build:js && npm run build:svg && npm run build:img && npm run migration",
    "migration": "node migration/migration.js",
    "local": "sudo NODE_ENV=local nodemon --ext js --ignore components/client-js/ --watch components/ --watch index.js index.js",
    "dev:be": "sudo NODE_ENV=development DEBUG=mai:* nodemon --ext js --ignore components/client-js/ --watch components/ --watch index.js index.js",
    "dev:fe": "npm run watch:reload & npm run watch:css & npm run watch:js & npm run watch:svg",
    "watch:reload": "DEBUG=wiper wiper -p 30001 -w public/dev/app.css,public/dev/app.js --cert=ssl.crt --key=ssl.key",
    "watch:css": "DEBUG=onchange onchange components/client-less/\\*\\*/\\*.less components/client-less/\\*.less -- npm run build:css:dev",
    "watch:js": "DEBUG=onchange onchange components/client-js/\\*\\*/\\*.js components/client-js/\\*.js components/builders/\\*.js components/templates/\\*\\*/\\*.js components/templates/\\*.js components/i18n/\\*.js -- npm run build:js:dev",
    "watch:svg": "DEBUG=onchange onchange components/client-svg/\\*\\*/\\*.svg -- npm run build:svg",
    "build:css": "lessc components/client-less/main.less | autoprefixer | cleancss -o public/assets/app.css && lessc components/client-less/inline.less | autoprefixer | cleancss -o public/assets/inline.css",
    "build:css:dev": "lessc components/client-less/main.less | autoprefixer > public/dev/app.css",
    "build:js": "browserify components/client-js/main.js -s mai | uglifyjs > public/assets/app.js && uglifyjs components/client-js/vendor/analytics.js > public/assets/vendor.js && uglifyjs components/client-js/vendor/inline.js > public/assets/inline.js",
    "build:js:dev": "browserify components/client-js/main.js -s mai -o public/dev/app.js",
    "build:svg": "svgo --enable=removeDesc --multipass -f components/client-svg/ && svgshelf components/client-svg/\\*.svg public/assets/icons.svg",
    "build:img": "find ./public/images/ -maxdepth 1 -type f -not -name .gitignore -exec rm -fv {} + && imagemin -p components/client-image/* public/images/",
    "cron:cache:all": "find ./cache/ -maxdepth 1 -type f -not -name .gitignore -exec rm -fv {} + | wc -w",
    "cron:cache:old": "find ./cache/ -maxdepth 1 -type f -mtime +7 -not -name .gitignore -exec rm -fv {} + | wc -w",
    "cron:ap:all": "node cron/action-point-recovery.js",
    "review:build": "browserify components/client-js/main.js -s app --list",
    "review:code": "eslint components/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitinn/mai.git"
  },
  "keywords": [
    "mai",
    "beginhq"
  ],
  "author": "David Frank",
  "license": "proprietary",
  "private": true,
  "bugs": {
    "url": "https://github.com/bitinn/mai/issues"
  },
  "homepage": "https://github.com/bitinn/mai",
  "devDependencies": {
    "autoprefixer": "5.2.0",
    "babel": "5.5.8",
    "babel-istanbul": "0.2.4",
    "browserify": "10.2.4",
    "browserify-shim": "3.8.8",
    "chai": "3.0.0",
    "clean-css": "3.3.2",
    "co-mocha": "1.1.0",
    "eslint": "0.23.0",
    "imagemin": "3.2.0",
    "less": "2.5.1",
    "mocha": "2.2.5",
    "nodemon": "1.3.7",
    "onchange": "1.1.0",
    "sinon": "1.15.3",
    "sinon-chai": "2.8.0",
    "svgo": "0.5.2",
    "svgshelf": "1.0.3",
    "uglify-js": "2.4.23",
    "wiper": "1.2.0"
  },
  "dependencies": {
    "cheerio": "0.19.0",
    "co": "4.5.4",
    "csrf": "3.0.0",
    "cuid": "1.2.5",
    "debug": "2.2.0",
    "deep-extend": "0.4.0",
    "dom-delegator": "13.1.0",
    "escape-string-regexp": "1.0.3",
    "grant-koa": "3.3.3",
    "he": "0.5.0",
    "icepick": "0.4.1",
    "inline-elements": "1.0.0",
    "js-beautify": "1.5.6",
    "koa": "0.21.0",
    "koa-bodyparser": "2.0.0",
    "koa-flash": "1.0.0",
    "koa-livereload": "0.1.23",
    "koa-logger": "bitinn/logger#local",
    "koa-mount": "1.3.0",
    "koa-router": "5.1.2",
    "koa-sendfile": "1.1.1",
    "koa-session": "3.2.0",
    "koa-static": "1.4.9",
    "lazysizes": "1.1.3",
    "lgtm": "1.0.4",
    "mime-types": "2.1.2",
    "mixpanel": "0.2.1",
    "mz": "2.0.0",
    "native-promise-only": "0.8.0-a",
    "node-fetch": "1.3.0",
    "node-polyglot": "0.4.3",
    "purest": "1.2.0",
    "redis": "0.12.1",
    "sharp": "0.10.1",
    "svg-tags": "1.0.0",
    "then-redis": "1.3.0",
    "tiny-emitter": "1.0.0",
    "validator": "3.40.1",
    "vdom-parser": "1.0.2",
    "vdom-to-html": "2.1.1",
    "virtual-dom": "2.0.1",
    "whatwg-fetch": "0.9.0",
    "xss-filters": "1.2.2",
    "xtend": "4.0.0",
    "yieldb": "2.6.0"
  },
  "engines": {
    "iojs": ">= 1.8.0",
    "node": ">= 0.12.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "./components/client-js/vendor/svg4everybody.js": ""
  },
  "eslintConfig": {
    "rules": {
      "quotes": [
        1,
        "single"
      ],
      "no-use-before-define": [
        1,
        "nofunc"
      ],
      "camelcase": 0,
      "semi": 0,
      "no-extra-semi": 0,
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-unused-expressions": 0,
      "strict": 0
    },
    "env": {
      "node": true,
      "es6": true,
      "browser": true,
      "mocha": true
    }
  }
}
